#! /usr/bin/env bash

# Installs Homebrew software.

if ! command -v brew > /dev/null; then
  /usr/bin/ruby -e "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/master/install)"
fi


brew install bat
brew install direnv
brew install fzf
brew install git
brew install httpie
brew install icdiff
brew install jq
brew install mas
brew install nvim
brew install zsh
brew install zsh-syntax-highlighting
